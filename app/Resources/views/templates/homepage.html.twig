{% extends "master.html.twig" %}

{% block meta %}
    {% autoescape false %}
        {{ sulu_seo(extension.seo, content, urls, shadowBaseLocale) }}
    {% endautoescape %}
{% endblock %}
{% block style %}
    <link rel="stylesheet" href="/css/homepage.css">
    <script src="/js/homepage.js"></script>
{% endblock %}
{% block header %}{% endblock %}
{% block bodyAttr %}
    {% if content.img_desktop and content.img_desktop['0'] %}
        {% set img_desktop =content.img_desktop['0'].url %}
    {% else %}
        {% set img_desktop ="/image/home.jpg" %}
    {% endif %}
    {% if content.img_mobile and content.img_mobile['0'] %}
        {% set img_mobile =content.img_mobile['0'].url %}
    {% else %}
        {% set img_mobile ="/image/home_mobile.jpg" %}
    {% endif %}
    data-desktop ="{{ img_desktop }}" data-mobile="{{ img_mobile }}" class="homepage"
{% endblock %}
{% block content %}

{% endblock %}
